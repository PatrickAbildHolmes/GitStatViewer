generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model RepoCommit {
  id         Int      @id @default(autoincrement())
  sha        String   @unique
  repo       String  // "owner/repo", not just "owner" or "repo".
  author     String // commit author, not owner
  timestamp  DateTime
  additions  Int?
  deletions  Int?
}


